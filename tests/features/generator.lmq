function fibonacci() {
  let last = 0;
  let current = yield 1;

  while (true) { 
    current = last + current;
    last = current - last;
    yield current;
  }
}

let generator = fibonacci();
println(generator);
let i = 1;
while (i <= 46) {
  generator.next();
  println("Fibonacci number at index ${i}: ${generator.value()}");
  i++;
}
