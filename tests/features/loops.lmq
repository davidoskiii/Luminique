println("While loop");

let i = 0;

while (i < 15) {
  if (i == 11) {
    break;
  } else if (i == 3 || i == 6) {
    i++;
    continue;
  }

  if (i != 10) {
    print("${i}, ");
  } else {
    println(i);
  }

  i++;
}

println("");
println("For loop");

for (let i = 0; i < 15; i++) {
  if (i == 11) {
    break;
  } else if (i == 3 || i == 6) {
    continue;
  }

  if (i != 10) {
    print("${i}, ");
  } else {
    println(i);
  }
}

println("");
println("Do-while loop");

i = 0;
do {
