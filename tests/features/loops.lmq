println("\nRange based for loop:");

println("\nArray:");

let arr = [1, 2, 3, 4];

for (value, index : arr) {
  println("value: ${value}, index: ${index}");
}

println("\nDictionary:");

let dict = {1: "one", 2: "two"};

for (value, index : dict) {
  println("value: ${value}, index: ${index}");
}
