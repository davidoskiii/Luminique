enum Animals {
  DOG, // 0
  CAT, // 1 
  HORSE, // 2
  SHRIMP // 3
}

println("Creating Enum from keyword: ${Animals}");

println("Animals::DOG: ${Animals::DOG}");
println("Animals::CAT: ${Animals::CAT}");
println("Animals::HORSE: ${Animals::HORSE}");
println("Animals::SHRIMP: ${Animals::SHRIMP}");

println("Typeof Animals:  ${typeof Animals}");

let Shops = Enum("Shops");
println("Creating Enum from class: ${Shops}");
