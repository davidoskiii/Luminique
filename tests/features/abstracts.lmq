abstract class Animal {
  function sleep() {
    println("Zzz...");
  }

  abstract animalSound();
}

class Dog : Animal {
  function animalSound() {
    println("The dog says: wof wof");
  }

  abstract favouriteActivity();
}

const dog = Dog();
dog.animalSound();
dog.sleep();

