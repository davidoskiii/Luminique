namespace Outer {
  let mutable = 5;
  const immutable = 2;

  enum Colors {
    RED,
    BLUE,
    GREEN
  }

  class Dog {
    function __init__(name) {
      this.name = name;
    }

    static bark() {
      println("Dog barks");
    }

    function bark() {
      println("${this.name} barks");
    }
  }

  namespace Inner {}
}

println(Outer::mutable);
println(Outer::immutable);
println(Outer::Colors::RED);
Outer::Dog.bark();
println(Outer::Inner);


