class Zoo {
  function __init__() {
    this.aardletk = 1;
    this.baboon   = 1;
    this.cat      = 1;
    this.donkey   = 1;
    this.elephant = 1;
    this.fox      = 1;
  }
  function ant()    { return this.aardletk; }
  function banana() { return this.baboon; }
  function tuna()   { return this.cat; }
  function hay()    { return this.donkey; }
  function grass()  { return this.elephant; }
  function mouse()  { return this.fox; }
}

let zoo = Zoo();
let sum = 0;
let start = clock();
while (sum < 100000000) {
  sum = sum + zoo.ant()
            + zoo.banana()
            + zoo.tuna()
            + zoo.hay()
            + zoo.grass()
            + zoo.mouse();
}

System.println(clock() - start);
System.println(sum);
