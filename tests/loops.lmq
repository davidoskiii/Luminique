println("While loop");

let i = 0;

while (i < 15) {
  if (i == 11) {
    break;
  } else if (i == 3 || i == 6) {
    i = i + 1;
    continue;
  }

  if (i != 10) {
    print("${i}, ");
  } else {
    println(i);
  }

  i = i + 1;
}

println("");
println("For loop");

for (let i = 0; i < 15; i = i + 1) {
  if (i == 11) {
    break;
  } else if (i == 3 || i == 6) {
    continue;
  }

  if (i != 10) {
    print("${i}, ");
  } else {
    println(i);
  }
}

println("\nRange based for loop:");

println("\nArray:");

let arr = [1, 2, 3, 4];

for (value, index : arr) {
  println("value: ${value}, index: ${index}");
}

println("\nDictionary:");

let dict = {"1": "one", "2": "two"};

for (value, key : dict) {
  println("value: \"${value}\", key: \"${key}\"");
}
