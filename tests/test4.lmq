class Scone {
  function topping(first, second) {
    print("scone with " + first + " and " + second);
  }
}

let scone = Scone();
scone.topping("berries", "cream");

class CoffeeMaker {
  function __init__(coffee) {
    this.coffee = coffee;
  }

  function brew() {
    print("Enjoy your cup of " + this.coffee);

    this.coffee = nil;
  }
}

let maker = CoffeeMaker("coffee and chicory");
maker.brew();
